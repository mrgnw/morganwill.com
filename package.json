{
	"name": "morg.info",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"generate-qr": "node scripts/generate-qr-codes.mjs",
		"build": "vite build",
		"preview": "vite preview",
		"prev": "vite build && vite preview",
		"cfb": "export $(cat .env | grep -v '^#' | xargs) && bun run build && wrangler versions upload --preview-alias $(git branch --show-current | sed 's/[^a-zA-Z0-9-]/-/g')",
		"deploy": "export $(cat .env | grep -v '^#' | xargs) && bun run build && wrangler deploy",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:perf": "vitest run tests/performance-metrics.test.js tests/page-load.test.js"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.302",
		"@sveltejs/vite-plugin-svelte": "^4.0.1",
		"@vitest/ui": "^4.0.16",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.21.16",
		"happy-dom": "^20.0.11",
		"iconify-icon": "^2.1.0",
		"mdsvex": "^0.11.2",
		"postcss": "^8.4.49",
		"postcss-load-config": "^5.1.0",
		"shiki": "^3.6.0",
		"shiki-transformer-copy-button": "^0.0.3",
		"svelte": "^5.46.0",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.15",
		"vite": "^5.4.11",
		"vitest": "^4.0.16",
		"wrangler": "4"
	},
	"type": "module",
	"dependencies": {
		"@iconify/svelte": "^4.2.0",
		"@iconify/utils": "^2.2.1",
		"@sveltejs/adapter-cloudflare": "^4.7.4",
		"@sveltejs/kit": "^2.8.1",
		"clsx": "^2.1.1",
		"lucide-svelte": "^0.460.1",
		"qrcode": "^1.5.4",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"unplugin-icons": "^0.19.3"
	}
}
